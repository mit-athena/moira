# $Id$

SHELL=/bin/sh

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(CPPFLAGS) $(CFLAGS) $(DEFS)
RANLIB=@RANLIB@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
BUILDTOP=../..

OBJS=error.o mail.o member.o pobox.o utils.o

.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

all: libmrclient.a

clean:
	rm -f $(OBJS) libmrclient.a

cleandir distclean: clean
	rm -f Makefile

depend:

install: all

libmrclient.a: $(OBJS)
	ar cru $@ $(OBJS)
	$(RANLIB) $@
