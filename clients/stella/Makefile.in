# $Id: Makefile.in,v 1.2 2009-01-05 22:25:31 zacheiss Exp $

SHELL=/bin/sh
@SET_MAKE@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(CPPFLAGS) $(CFLAGS) $(DEFS)
LDFLAGS=@LDFLAGS@
LIBS=../lib/libmrclient.a @LIBS@
MR_LIBDEP=@MR_LIBDEP@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
BUILDTOP=../..
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

OBJS=stella.o

TARGET=stella

.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

all: $(TARGET)

clean:
	rm -f $(OBJS) $(TARGET)

cleandir distclean: clean
	rm -f Makefile

depend:

install: all
	$(INSTALL_PROGRAM) stella $(DESTDIR)$(bindir)

$(TARGET): $(OBJS) ../lib/libmrclient.a $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(LIBS)
