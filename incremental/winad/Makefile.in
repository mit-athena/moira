# $Id: Makefile.in,v 1.5 2009-06-01 21:05:01 zacheiss Exp $

SHELL=/bin/sh
@SET_MAKE@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
LDAP_CPPFLAGS=@LDAP_CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(LDAP_CPPFLAGS) $(CPPFLAGS) $(CFLAGS) $(DEFS)
LIBTOOL=@LIBTOOL@
LDFLAGS=@LDFLAGS@
LDAP_LIBS=@LDAP_LIBS@
LIBS=@LIBS@
MR_LIBDEP=@MR_LIBDEP@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
top_builddir=@top_builddir@
BUILDTOP=../..
mrbindir=@mrbindir@

WINAD_OBJS=winad.lo ldap_adgssapi_bind.lo krb5_utils.lo setpw.lo

TARGET=winad.incr

.SUFFIXES: .pc .lo

.pc.c:
	$(PRO_C) $(PRO_C_FLAGS) INAME=$<

.c.lo:
	$(LIBTOOL) --mode=compile $(CC) -c $(ALL_CFLAGS) $<

all: $(TARGET)

clean:
	$(LIBTOOL) --mode=clean rm -f $(WINAD_OBJS) $(TARGET)

cleandir distclean: clean
	rm -f Makefile

depend:

install:
	$(LIBTOOL) --mode=install $(INSTALL_PROGRAM) winad.incr $(DESTDIR)$(mrbindir)

winad.incr: $(WINAD_OBJS) $(MR_LIBDEP)
	$(LIBTOOL) --mode=link $(CC) -o $@ $(LDAP_LIBS) $(LDFLAGS) $(WINAD_OBJS) -lldap -llber \
	-lgssapi_krb5 $(LIBS)
