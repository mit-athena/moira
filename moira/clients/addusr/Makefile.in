# $Id: Makefile.in,v 1.5 1999-05-13 18:53:28 danw Exp $

SHELL=/bin/sh
@SET_MAKE@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(CPPFLAGS) $(CFLAGS) $(DEFS)
LDFLAGS=@LDFLAGS@
LIBS=../lib/libmrclient.a @LIBS@
MR_LIBDEP=@MR_LIBDEP@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
BUILDTOP=../..
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

OBJS=addusr.o

TARGET=addusr

.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

all: depend $(TARGET)

clean:
	rm -f $(OBJS) $(TARGET)

depend:

install: all
	$(INSTALL_PROGRAM) -m 755 addusr $(bindir)

$(TARGET): $(OBJS) ../lib/libmrclient.a $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(LIBS)
