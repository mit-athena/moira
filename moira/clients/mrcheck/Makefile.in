# $Id: Makefile.in,v 1.3 1998-08-27 17:13:25 danw Exp $

SHELL=/bin/sh
@SET_MAKE@

CC=@CC@
CPPFLAGS=@CPPFLAGS@
CFLAGS=@CFLAGS@
DEFS=@DEFS@
ALL_CFLAGS=$(CPPFLAGS) $(CFLAGS) $(DEFS)
LDFLAGS=@LDFLAGS@
LIBS=@LIBS@
MR_LIBDEP=@MR_LIBDEP@
INSTALL=@INSTALL@
INSTALL_PROGRAM=@INSTALL_PROGRAM@

srcdir=@srcdir@
VPATH=@srcdir@
SRCTOP=@top_srcdir@
BUILDTOP=../..
prefix=@prefix@
exec_prefix=@exec_prefix@
bindir=@bindir@

OBJS=mrcheck.o

TARGET=mrcheck

.c.o:
	$(CC) -c $(ALL_CFLAGS) $<

all: $(TARGET)

clean:
	rm -f $(OBJS) $(TARGET)

depend:

install: all
	$(INSTALL_PROGRAM) mrcheck $(bindir)

$(TARGET): $(OBJS) $(MR_LIBDEP)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(LIBS)
