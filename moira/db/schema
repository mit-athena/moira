	allocated	INTEGER 	NOT NULL WITH DEFAULT,
	partsize	INTEGER 	NOT NULL WITH DEFAULT,
	modtime		DATE    	NOT NULL WITH DEFAULT,
	modby		INTEGER 	NOT NULL WITH DEFAULT,
	modwith		CHAR(8) 	NOT NULL WITH DEFAULT
);
